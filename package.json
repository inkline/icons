{
    "name": "@inkline/icons",
    "description": "Inkline Icons provides you with an awesome, maintainable Universal Vue.js 3 Icon Component with thousands of icons from more than 15 icon packs.",
    "version": "1.4.12",
    "iconsVersion": "1.0.0",
    "main": "index.js",
    "module": "index.js",
    "types": "index.d.ts",
    "scripts": {
        "dist:plugin": "vite build",
        "dist:packs": "node -r esbuild-register scripts/dist.ts",
        "dist": "npm run build && npm run dist:plugin && npm run dist:packs",
        "build:ts:compile": "tsc -d -p tsconfig.json && tsconfig-replace-paths --project tsconfig.json",
        "build:ts:lib:copy": "bash -c 'rsync -am --include=\"*.\"{vue,scss} --include=\"*/\" --exclude=\"*\" src/ lib/'",
        "build:ts:lib:replace": "bash -c 'find lib -type f -name *.vue -exec perl -i -pe\"s/.ts\\\" lang=\\\"ts\\\"/.js\\\"/g\" {} +'",
        "build:ts:lib": "npm run build:ts:lib:copy && npm run build:ts:lib:replace",
        "build:ts": "npm run build:ts:compile && npm run build:ts:lib",
        "build": "npm run generate && npm run build:ts",
        "generate:version": "node -r esbuild-register scripts/version.ts",
        "generate:icons": "node -r esbuild-register scripts/generate.ts",
        "generate:exports": "node -r esbuild-register scripts/exports.ts",
        "generate": "npm run generate:version && npm run generate:icons",
        "prerelease": "npm run dist && node -r esbuild-register scripts/prepublish.ts",
        "release": "np",
        "postrelease": "node -r esbuild-register scripts/postpublish.ts",
        "postversion": "node -r esbuild-register scripts/postversion.ts",
        "test": "jest",
        "test:dev": "jest --watch"
    },
    "peerDependencies": {
        "vue": ">=3.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.14.0",
        "@babel/preset-env": "^7.14.1",
        "@babel/preset-typescript": "^7.13.0",
        "@coreui/icons": "2.0.1",
        "@fortawesome/fontawesome-free": "5.15.3",
        "@primer/octicons": "13.0.0",
        "@testing-library/jest-dom": "^5.12.0",
        "@types/estree": "0.0.47",
        "@types/fs-extra": "^9.0.12",
        "@types/glob": "^7.1.3",
        "@types/jest": "^26.0.23",
        "@types/node": "^15.0.1",
        "@typescript-eslint/eslint-plugin": "^4.22.0",
        "@typescript-eslint/parser": "^4.22.0",
        "@vitejs/plugin-vue": "^1.2.2",
        "@vue/compiler-sfc": "^3.0.11",
        "@vue/test-utils": "^2.0.0-rc.6",
        "babel-jest": "^26.6.3",
        "bootstrap-icons": "1.4.1",
        "chalk": "^4.1.1",
        "cryptocurrency-icons": "0.17.1",
        "esbuild-register": "^2.5.0",
        "eslint": "^7.25.0",
        "eslint-plugin-vue": "^7.9.0",
        "flag-icon-css": "3.5.0",
        "flat-color-icons": "1.1.0",
        "fs-extra": "^10.0.0",
        "glob": "^7.1.6",
        "heroicons": "1.0.1",
        "ionicons": "5.5.1",
        "jest": "^26.6.3",
        "line-awesome": "1.3.0",
        "material-design-icons": "3.0.1",
        "pixelarticons": "1.4.0",
        "primeicons": "4.1.0",
        "remixicon": "2.5.0",
        "sass": "^1.32.12",
        "simple-icons": "4.21.0",
        "stylelint": "^13.13.1",
        "stylelint-config-standard": "^22.0.0",
        "stylelint-scss": "^3.19.0",
        "svgson": "^5.2.1",
        "ts-jest": "^26.5.6",
        "ts-node": "^9.1.1",
        "tsconfig-replace-paths": "^0.0.5",
        "typescript": "^4.1.3",
        "vite": "^2.2.3",
        "vue": "^3.0.5",
        "vue-jest": "^5.0.0-alpha.9",
        "vue-tsc": "^0.0.24"
    },
    "files": [
        "**/*"
    ],
    "keywords": [
        "vuejs",
        "universal",
        "icon",
        "component",
        "icons",
        "packs",
        "inkline",
        "coreui",
        "cryptocurrency",
        "flag",
        "fontawesome",
        "heroicons",
        "bootstrap",
        "flat color",
        "ionicons",
        "lineawesome",
        "material design",
        "octicons",
        "pixelart",
        "prime",
        "remix",
        "simple"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/inkline/icons"
    },
    "license": "MIT"
}
