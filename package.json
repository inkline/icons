        "build",
        "dist",
        "src",
        "scripts",
        "README.md",
        "LICENSE",
        "tsconfig.json"
    ],
    "keywords": [
        "vuejs",
        "universal",
        "icon",
        "component",
        "icons",
        "packs",
        "inkline",
        "coreui",
        "cryptocurrency",
        "flag",
        "fontawesome",
        "heroicons",
        "bootstrap",
        "flat color",
        "ionicons",
        "lineawesome",
        "material design",
        "octicons",
        "pixelart",
        "prime",
        "remix",
        "simple"
    ],
