{
    "name": "@inkline/icons",
    "description": "Inkline Icons provides you with an awesome, maintainable Universal Vue.js 3 Icon Component with thousands of icons from more than 15 icon packs.",
    "version": "1.4.3",
    "iconsVersion": "1.0.0",
    "main": "build/index.js",
    "module": "./build/index.js",
    "types": "./build/index.d.ts",
    "scripts": {
        "dist:plugin": "vite build",
        "dist:packs": "node -r esbuild-register scripts/dist.ts",
        "dist": "npm run build && npm run dist:plugin && npm run dist:packs",
        "build:ts": "tsc -d -p tsconfig.json",
        "build:vue:copy": "bash -c 'rsync -am --include=\"*.\"{vue,scss} --include=\"*/\" --exclude=\"*\" src/ build/'",
        "build:vue:replace": "bash -c 'find build -type f -name *.vue -exec perl -i -pe\"s/.ts\\\" lang=\\\"ts\\\"/.js\\\"/g\" {} +'",
        "build:vue": "npm run build:vue:copy && npm run build:vue:replace",
        "build": "npm run generate && npm run build:ts && npm run build:vue",
        "generate:version": "node -r esbuild-register scripts/version.ts",
        "generate:icons": "node -r esbuild-register scripts/generate.ts",
        "generate:exports": "node -r esbuild-register scripts/exports.ts",
        "generate": "npm run generate:exports && npm run generate:version && npm run generate:icons",
        "prepublishOnly": "npm run build",
        "test": "jest",
        "test:dev": "jest --watch"
    },
    "peerDependencies": {
        "vue": "^3.0.5"
    },
    "devDependencies": {
        "@babel/core": "^7.14.0",
        "@babel/preset-env": "^7.14.1",
        "@babel/preset-typescript": "^7.13.0",
        "@coreui/icons": "2.0.1",
        "@fortawesome/fontawesome-free": "5.15.3",
        "@primer/octicons": "13.0.0",
        "@testing-library/jest-dom": "^5.12.0",
        "@types/estree": "0.0.47",
        "@types/glob": "^7.1.3",
        "@types/jest": "^26.0.23",
        "@types/node": "^15.0.1",
        "@typescript-eslint/eslint-plugin": "^4.22.0",
        "@typescript-eslint/parser": "^4.22.0",
        "@vitejs/plugin-vue": "^1.2.2",
        "@vue/compiler-sfc": "^3.0.11",
        "@vue/test-utils": "^2.0.0-rc.6",
        "babel-jest": "^26.6.3",
        "bootstrap-icons": "1.4.1",
        "chalk": "^4.1.1",
        "cryptocurrency-icons": "0.17.1",
        "esbuild-register": "^2.5.0",
        "eslint": "^7.25.0",
        "eslint-plugin-vue": "^7.9.0",
