  "types": "./build/index.d.ts",
  "scripts": {
    "dist:plugin": "vite build",
    "dist:packs": "node -r esbuild-register scripts/dist.ts",
    "dist": "npm run build && npm run dist:plugin && npm run dist:packs",
    "build:ts": "tsc -d -p tsconfig.json",
    "build:vue:copy": "bash -c 'rsync -am --include=\"*.\"{vue,scss} --include=\"*/\" --exclude=\"*\" src/ build/'",
    "build:vue:replace": "bash -c 'find build -type f -name *.vue -exec perl -i -pe\"s/.ts\\\" lang=\\\"ts\\\"/.js\\\"/g\" {} +'",
    "build:vue": "npm run build:vue:copy && npm run build:vue:replace",
    "build": "npm run generate && npm run build:ts && npm run build:vue",
    "generate:version": "node -r esbuild-register scripts/version.ts",
    "generate:icons": "node -r esbuild-register scripts/generate.ts",
    "generate": "npm run generate:version && npm run generate:icons",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "peerDependencies": {
    "vue": "^3.0.5"
  },
  "devDependencies": {
