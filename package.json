        "jest": "^26.6.3",
        "line-awesome": "1.3.0",
        "material-design-icons": "3.0.1",
        "pixelarticons": "1.4.0",
        "primeicons": "4.1.0",
        "remixicon": "2.5.0",
        "sass": "^1.32.12",
        "simple-icons": "4.21.0",
        "stylelint": "^13.13.1",
        "stylelint-config-standard": "^22.0.0",
        "stylelint-scss": "^3.19.0",
        "svgson": "^5.2.1",
        "ts-jest": "^26.5.6",
        "ts-node": "^9.1.1",
        "typescript": "^4.1.3",
        "vite": "^2.2.3",
        "vue": "^3.0.5",
        "vue-jest": "^5.0.0-alpha.9",
